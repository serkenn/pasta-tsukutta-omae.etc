# Discord bot token
BOT_TOKEN=your_bot_token_here
# (Optional) specify a guild id for registering commands quickly during development
GUILD_ID=your_guild_id_here
# Local audio trigger text and file
TRIGGER_TEXT=play_local
LOCAL_AUDIO=./audio/sample.mp3
# (Optional) Only allow controls to VC participants (default true)
REQUIRE_VC=true
# (Optional) loop artist setting - forced to 湘南乃風
ARTIST_NAME=湘南乃風
# Debug
NODE_ENV=development
